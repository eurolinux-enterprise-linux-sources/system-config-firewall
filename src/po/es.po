#: ../src/fw_parser.py:161 ../src/fw_parser.py:165
msgid "<module>"
msgstr "<módulo>"

#: ../src/fw_parser.py:162
msgid "Enable an iptables module"
msgstr "Habilitar un módulo iptables"

#: ../src/fw_parser.py:166
msgid "Disable an iptables module"
msgstr "Inhabilitando un módulo de iptables"

#: ../src/fw_parser.py:170
msgid "<service>"
msgstr "<service>"

#: ../src/fw_parser.py:171
msgid "Open the firewall for a service (e.g, ssh)"
msgstr "Abrir el cortafuegos para un servicio (por ejemplo, ssh)"

#: ../src/fw_parser.py:174
msgid "<port>[-<port>]:<protocol>"
msgstr "<puerto>[-<puerto>]:<protocolo>"

#: ../src/fw_parser.py:176
msgid "Open specific ports in the firewall (e.g, ssh:tcp)"
msgstr "Abrir puertos específicos en el cortafuegos (por ejemplo, ssh:tcp)"

#: ../src/fw_parser.py:180 ../src/fw_parser.py:184
msgid "<interface>"
msgstr "<interfase>"

#: ../src/fw_parser.py:181
msgid "Allow all traffic on the specified device"
msgstr "Permitir todo el tráfico en el dispositivo seleccionado"

#: ../src/fw_parser.py:185
msgid "Masquerades traffic from the specified device. This is IPv4 only."
msgstr ""
"Enmascarar el tráfico desde un dispositivo especificado. Esto es sólo para "
"IPv4."

#: ../src/fw_parser.py:189
msgid "Backwards compatibility, aliased to --enabled"
msgstr "Compatibilidad hacia atrás, similar a --enabled"

#: ../src/fw_parser.py:192
msgid "[<type>:][<table>:]<filename>"
msgstr "[<tipo>:][<tabla>:]<nombre_de_archivo>"

#: ../src/fw_parser.py:194
msgid ""
"Specify a custom rules file for inclusion in the firewall, after the default "
"rules. Default protocol type: ipv4, default table: filter. (Example: ipv4:"
"filter:/etc/sysconfig/ipv4_filter_addon)"
msgstr ""
"Especifique un archivo de reglas personalizadas para incluir en el "
"cortafuegos. Tipo por defecto: ipv4, tabla por defecto: filter. (Ejemplo: "
"ipv4:filter:/etc/sysconfig/ipv4_filter_addon)"

#: ../src/fw_parser.py:203
msgid ""
"if=<interface>:port=<port>:proto=<protocol>[:toport=<destination port>][:"
"toaddr=<destination address>]"
msgstr ""
"if=<interfase>:port=<puerto>:proto=<protocolo>[:toport=<puerto destino>][:"
"toaddr=<dirección destino>]"

#: ../src/fw_parser.py:207
msgid ""
"Forward the port with protocol for the interface to either another local "
"destination port (no destination address given) or to an other destination "
"address with an optional destination port. This is IPv4 only."
msgstr ""
"Reenvío de puerto con protocolo desde una interfase hacia otro puerto "
"destino (sin entregar dirección de destino) o hacia otra dirección destino "
"con un puerto de destino opcional. Esto es sólo para IPv4."

#: ../src/fw_parser.py:216
msgid "<icmp type>"
msgstr "<icmp type>"

#: ../src/fw_parser.py:217
msgid "Block this ICMP type. The default is to accept all ICMP types."
msgstr "Bloquear este tipo ICMP. Por defecto se aceptan todos los tipos ICMP."

#: ../src/fw_parser.py:223
msgid "Disable Internet Protocol Security (IPsec)"
msgstr "Inhabilitar el Protocolo de Seguridad en Internet (IPsec)"

#: ../src/fw_parser.py:226
msgid "Disable Internet Printing Protocol (IPP)"
msgstr "Inhabilitar el Protocolo de Impresión en Internet (IPP)"

#: ../src/fw_parser.py:229
msgid "Disable Multicast DNS (mDNS)"
msgstr "Inhabilitar el Multicast de DNS (mDNS)"

#: ../src/fw_parser.py:232
msgid "SELinux Options (deprecated)"
msgstr "Opciones de SELinux (obsoletas)"

#: ../src/fw_parser.py:233
msgid ""
"Using these options with no additional firewall options will not create or "
"alter firewall configuration, only SELinux will be configured."
msgstr ""
"Utilizar estas opciones solamente (sin otras opciones de cortafuegos "
"adicionales), no generará ni modificará la configuración del cortafuegos: "
"solo SELinux será configurado.  "

#: ../src/fw_parser.py:239
msgid "<mode>"
msgstr "<modo>"

#: ../src/fw_parser.py:240
#, python-format
msgid "Configure SELinux mode: %s"
msgstr "Configure el modo de SELinux: %s"

#: ../src/fw_parser.py:244 ../src/fw_parser.py:404 ../src/fw_parser.py:457
msgid "<type>"
msgstr "<tipo>"

#: ../src/fw_parser.py:245
msgid "Configure SELinux type: Usually targeted or strict Policy"
msgstr "Configurar el tipo SELinux: usualmente política confinada o estricta"

#: ../src/fw_parser.py:259
#, python-format
msgid "no such option: %s"
msgstr "no existe la opción: %s"

