#, python-format
msgid "invalid port definition %s."
msgstr "définition de port « %s » invalide."

#: ../src/fw_parser.py:67
msgid "ipv6 has no nat support."
msgstr "ipv6 ne prend pas en charge NAT."

#: ../src/fw_parser.py:73
#, python-format
msgid "invalid service '%s'."
msgstr "service « %s » invalide."

