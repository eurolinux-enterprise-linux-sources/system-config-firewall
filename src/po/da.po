#, python-format
msgid "Interface '%s' is not valid."
msgstr "Grænseflade \"%s\" er ikke gyldig."

#: ../src/fw_tui.py:488
#, python-format
msgid "Address '%s' is not valid."
msgstr "Adresse \"%s\" er ikke gyldig."

#: ../src/fw_tui.py:521 ../src/system-config-firewall.glade.h:75
msgid ""
"Please select the protocol type, the firewall table and the file containing "
"the rules."
msgstr ""
"Vælg venligst protokoltypen, firewalltabellen og filen der indeholder "
"reglerne."

#: ../src/fw_tui.py:523
msgid "Firewall Table"
msgstr "Firewalltabel"

#: ../src/fw_tui.py:524
msgid "File"
msgstr "Fil"

#: ../src/fw_tui.py:533
#, python-format
