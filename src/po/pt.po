#, python-format
msgid "Address '%s' is not valid."
msgstr "O endereço '%s' não é válido."

#: ../src/fw_tui.py:521 ../src/system-config-firewall.glade.h:75
msgid ""
"Please select the protocol type, the firewall table and the file containing "
"the rules."
msgstr ""
"Por favor, seleccione o tipo de protocolo, a tabela da firewall e o ficheiro "
"com as regras."

#: ../src/fw_tui.py:523
msgid "Firewall Table"
msgstr "Tabela da Firewall"

#: ../src/fw_tui.py:524
msgid "File"
msgstr "Ficheiro"

